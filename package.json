{
	"name": "file-id-generator",
	"version": "0.6.0",
	"description": "Inserts a unique md5 hash into the selected file. Useful for creating unique IDs for your project files.",
	"keywords": [
		"md5",
		"path",
		"file",
		"id",
		"generator",
		"comment",
		"hash"
	],
	"license": "MIT",
	"main": "./lib/file-id-generator",
	"repository": "https://github.com/macaqueio/file-id-generator.git",
	"scripts": {
		"eslint-fix": "eslint --fix lib/",
		"eslint-check": "eslint lib/"
	},
	"dependencies": {
		"md5": "^2.2.1"
	},
	"devDependencies": {
		"babel-eslint": "^6.1.2",
		"eslint": ">=4.18.2",
		"eslint-config-atom-build": "^4.0.0"
	},
	"engines": {
		"atom": ">=1.0.0 <2.0.0"
	},
	"activationCommands": {
		"atom-workspace": "file-id-generator:generate"
	}
}
